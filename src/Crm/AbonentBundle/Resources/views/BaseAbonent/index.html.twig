{% extends '::base.html.twig' %}

{% block body -%}
    <h1>BaseAbonent list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Balance</th>
                <th>Archive</th>
                <th>Numdogovor</th>
                <th>Status</th>
                <th>Phonecont</th>
                <th>Email</th>
                <th>Name</th>
                <th>Lang</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('baseabonent_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.balance }}</td>
                <td>{{ entity.archive }}</td>
                <td>{{ entity.numDogovor }}</td>
                <td>{{ entity.status }}</td>
                <td>{{ entity.phoneCont }}</td>
                <td>{{ entity.email }}</td>
                <td>{{ entity.name }}</td>
                <td>{{ entity.lang }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('baseabonent_show', { 'id': entity.id }) }}">show</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    {% endblock %}
